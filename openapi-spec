        type: string
      first_name: &id012
        description: First name
        type: string
      last_name: &id013
        description: Last name
        type: string
      name_suffix: &id014
        description: Name suffix
        type: string
      phone_numbers: &id015
        description: List of phone numbers. At least one must be a MOBILE number.
        items:
          $ref: '#/definitions/phone_number'
        type: array
      postal_code: *id009
      state: *id010
    required:
    - first_name
    - last_name
    - email
    - phone_numbers
    - address1
    - city
    - state
    - postal_code
    - country_code
    type: object
