        description: ID of the parent item
        readOnly: true
        type: string
      price:
        example: &id041
          last_updated: '2020-05-06T19:34:01.963Z'
          list_price: 10.99
          map_price: 9.99
          offer_price: 9.99
        properties: &id042
          last_updated:
            description: Date this price was last updated
            format: date-time
            readOnly: true
            type: string
          list_price:
            description: 'List price for this item. Example: 4.99'
            format: float
            type: number
          map_price:
            description: 'Minimum Advertised Price (MAP) price for this item. Example:
              2.99'
            format: float
            type: number
          offer_price:
            description: 'Offer price for this item. Example: 4.99. If offer price
              is sent it must be less than or equal to list price'
            format: float
            type: number
        readOnly: true
        required: &id043
        - list_price
        type: object
      product_statuses:
        description: Product statuses for this product. Will be limited to at most
          two statuses, latest and current, for response bodies. Retrievals can expand
          to the full list using expand=product_statuses.
        items: &id044
          $ref: '#/definitions/product_status'
        readOnly: true
        type: array
      quantities:
        description: Quantities for this product
        items:
          $ref: '#/definitions/product_quantity'
        readOnly: true
        type: array
