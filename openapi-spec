        description: Flag indicating if this is the current/live version
        readOnly: true
        type: boolean
      errors:
        description: List of errors for this status version. When updating a status,
          including null will keep any existing errors. Include an empty list to remove
          all errors.
        items:
          $ref: '#/definitions/product_error'
        type: array
