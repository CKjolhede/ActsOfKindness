  transfer:
    description: Transfer object coming from payments processor
    example:
      currency: USD
      external_id: ex_12345678
      id: 123abc
      order_id: 123-abc
      order_number: '123'
      payment_id: 456def
      payment_timestamp: ''
      product_amount: 10.0
      quantity: 1
      referral_percentage: 0.1
      seller_product_amount: 3.0
      seller_service_charge_amount: 1.0
      seller_tax_amount: 3.0
      ship_advice_number: abc
      target_referral_amount: 2.0
      target_service_charge_amount: 1.0
      target_tax_amount: 2.0
      tcin: '12345678'
      total_payment_amount: 10.0
    properties:
      currency:
        description: Currency used (USD,EUR... ISO 4217)
        enum:
        - USD
        - GBP
        - YEN
        type: string
      external_id:
        description: External SKU for this item
        type: string
      id:
        description: Transfer ID. Also can use this for paging using the results_offset_id
          parameter.
        type: string
      order_id:
        description: ID for the order
        type: string
      order_number:
        description: Order number
        type: string
      payment_id:
        description: ID for the associated payment object
        type: string
      payment_timestamp:
        description: Timestamp when this transaction was created
        format: date-time
        type: string
      product_amount:
        description: Total charge for the product, including modifiers, minus shipping
        type: number
      quantity:
        description: Quantity for this order line
        type: integer
      referral_percentage:
        description: Percentage going to target
        type: number
      seller_product_amount:
        description: Amount from the product sale going to the seller
        type: number
      seller_service_charge_amount:
        description: Portion of service charge going to seller
        type: number
      seller_tax_amount:
        description: Taxes to be paid by the seller
        type: number
      ship_advice_number:
        description: Ship advice number
        type: string
      target_referral_amount:
        description: Actual amount from the product sale going to target
        type: number
      target_service_charge_amount:
        description: Portion of service charge going to Target
        type: number
      target_tax_amount:
        description: Taxes to be paid by target
        type: number
      tcin:
        description: Target Corporation Item Number (TCIN) for the item on this order
          line
        type: string
      total_payment_amount:
        description: Total amount of the transfer to the seller. total = product amount
          + service charge + taxes
        type: number
    type: object
