        description: Severity of the error
        enum:
        - NON_CRITICAL
        - CRITICAL
        type: string
      last_modified: *id003
      last_modified_by: *id004
      reason: &id025
        description: Description of the error
        type: string
      status:
        description: Status of the listing error
        enum:
        - ACTIVE
        - DELETED
        type: string
    type: object
  listing_error_category_list:
