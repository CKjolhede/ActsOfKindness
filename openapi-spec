      max_score: 1
      min_score: 0.5
      must_be_on_required: true
      resulting_status: REVIEW
      seller_id: def456
    properties:
      allow_new_versions:
        default: false
        description: Whether or not to allow new versions if this rule applies.
        type: boolean
      created: *id001
      created_by: *id002
      error_code:
        description: 'The code describes a specific type of error that can be made
          when entering a data field. This error code can refer to the document as
          a whole, like ''MISSING_FIELDS,'' or to an issue with a specific field,
          like ''MISSING_BRAND.'' Error codes are maintained and updated in the following
          Confluence page: https://confluence.target.com/display/CHACO/Errors+and+Warnings+Manifest

          '
        type: string
      id:
        description: ID of the product validation rule
        readOnly: true
        type: string
      last_modified: *id003
      last_modified_by: *id004
      max_score:
        description: 'Some product feedback coming from upstream will contain scores
          (e.g. risk level for questionable content). This value specifies the maximum
          value for the score, inclusive, for this rule to match. The value is optional,
          and if not provided the the range will be unbounded on the high side.

          '
        format: float
        type: number
      min_score:
        description: 'Some product feedback coming from upstream will contain scores
          (e.g. risk level for questionable content). This value specifies the minimum
          value for the score, inclusive, for this rule to match. The value is optional,
          and if not provided the the range will be unbounded on the low side.

          '
        format: float
        type: number
      must_be_on_required:
        description: If true, the rule should only apply to required fields; otherwise,
          the rule applies to any field.
        type: boolean
      resulting_status:
        description: Resulting product validation status should this rule apply.
        enum:
        - ERROR
        - BLOCKED
        - REVIEW
        type: string
