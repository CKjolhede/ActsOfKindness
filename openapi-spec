basePath: /sellers/v1
consumes:
- application/json
definitions:
  address:
    example:
      address1: 1000 Nicollet Mall
      address2: Suite 100
      city: Minneapolis
      country_code: US
      postal_code: '55403'
      state: MN
    properties:
      address1: &id005
        description: Physical mailing address
        type: string
      address2: &id006
        description: Physical mailing address
        type: string
      city: &id007
        description: City
        type: string
      country_code: &id008
        description: ISO 3166-1 alpha-2 country code
        type: string
      postal_code: &id009
        description: Mailing code
        type: string
      state: &id010
        description: State or province abbreviation
        example: MN
        type: string
    required:
    - address1
    - city
    - state
    - postal_code
    - country_code
    type: object
  approval_status:
    description: Approval status for a Seller
    enum: &id059
    - APPLIED
    - BEING_VETTED
    - ONBOARDING
    - APPROVED_TO_LIST
    - SUSPENDED
    - DEFUNCT
    type: string
  approval_status_object:
    properties:
      approval_status:
        $ref: '#/definitions/approval_status'
    required:
    - approval_status
    type: object
  audited_resource:
    example:
      created: '2020-05-06T19:34:01.963Z'
      created_by: steve.smith@target.com
      last_modified: '2020-05-06T19:34:01.963Z'
      last_modified_by: steve.smith@target.com
    properties:
      created: &id001
        description: Date this resource was created
        format: date-time
        readOnly: true
        type: string
      created_by: &id002
        description: ID of user that created this resource
        readOnly: true
        type: string
      last_modified: &id003
        description: Date this resource was last modified
        format: date-time
        readOnly: true
        type: string
      last_modified_by: &id004
        description: ID of last user to modify this resource
        readOnly: true
        type: string
    type: object
  autoapproval_settings:
    example:
      enabled: true
      item_types_blacklist:
      - '123'
      - '456'
    properties:
      created: *id001
      created_by: *id002
      enabled:
        description: Whether this seller is enabled for autoapproval. Defaults to
          false
        type: boolean
      item_types_blacklist:
        items:
          type: string
        type: array
      last_modified: *id003
      last_modified_by: *id004
      seller_id:
        description: ID of the seller these settings pertain to
        readOnly: true
        type: string
    type: object
  bulk_request_order_fulfillments_create:
    description: Bulk request containing order fulfillments to create
    properties:
      items:
        description: List of order fulfillments
        items:
          $ref: '#/definitions/order_fulfillment_create'
        type: array
    type: object
  bulk_request_order_fulfillments_update:
    description: Bulk request containing order fulfillments to update
    properties:
      items:
        description: List of order fulfillments
        items:
          $ref: '#/definitions/order_fulfillment_bulk_update'
        type: array
    type: object
  bulk_request_product_variation:
    description: Bulk request containing a parent (relationship_type=VAP) and children
      (relationship_type=VC) products
    example:
      children:
      - external_id: ex_123
        fields:
        - name: size
          value: small
        relationship_type: VC
      parent:
        external_id: 123abc
        fields:
        - name: description
          value: a great shirt
        relationship_type: VAP
    properties:
      children:
        description: List of children products. May only contains VCs.
        items:
          $ref: '#/definitions/product_update'
        type: array
      parent:
        $ref: '#/definitions/product_update'
    type: object
  bulk_request_products:
    description: Bulk request containing product
    properties:
      items:
        description: List of product
        items:
          $ref: '#/definitions/product'
        type: array
    type: object
  bulk_response_order_fulfillments:
    description: Bulk response containing order fulfillments
    properties:
      results:
        description: List of products
        items:
          $ref: '#/definitions/order_fulfillment'
        type: array
    type: object
  bulk_response_product_update_results:
    description: Bulk response containing product update results
    properties:
      results:
        description: List of product update results
        items:
          $ref: '#/definitions/product_update_result'
        type: array
    type: object
  bulk_response_products:
    description: Bulk response containing product
    properties:
      results:
        description: List of products
        items:
          $ref: '#/definitions/product'
        type: array
    type: object
  carrier:
    description: Shipping carrior information
    example:
      id: 1
      shipping_account: acct-123
    properties:
      id:
        description: ID for this carrier, VMM master data.
        type: integer
      shipping_account:
        description: Shipping account number provided by seller
        type: string
    required:
    - id
    type: object
  case:
    example:
      attachment_ids:
      - 0a2f92a4c-disputes.xlsx
      case_id: ''
      case_number: ''
      case_type: ITEM_INQUIRY
      created: '2020-05-18T13:03:15Z'
      emails:
      - first.last@target.com
      issue_description: Issue description here
      last_modified: '2020-05-18T13:03:19Z'
      level1: Item
      level2: Inquiry
      origin: origin
      seller_email: first.last@target.com
      seller_id: 5ab01575a9bb3a05521bc8e4
      seller_order_number: 123abc
      status: Assigned
      subject: My subject
      vendor_id: '123'
    properties:
      attachment_ids:
        description: List of attachment ids for a case
        items:
          example: 0a2f92a4c-disputes.xlsx
          type: string
        type: array
      case_id:
        description: identifier of the case
        type: string
      case_number:
        description: The case id that is displayed in Salesforce
        type: string
      case_type:
        description: The type of case this is. combination of level1 & level2 values
          in salesforce
        enum: &id071
        - ITEM_INQUIRY
        - ITEM_INVENTORY
        - ITEM_PRICE
        - ITEM_ERRORS
        - FULFILLMENT_SHIPPING
        - FULFILLMENT_CANCEL
        - RETURNS_INQUIRY
        - RETURNS_DISPUTES
        - PAYMENT_INQUIRY
        - PAYMENT_DISPUTES
        - TARGET_INQUIRY
        - TARGET_MISSING_ITEM
        type: string
      created:
        description: Date this resource was created
        format: date-time
        readOnly: true
        type: string
      emails:
        description: List of emails for a case
        items:
          $ref: '#/definitions/case_email'
        type: array
      issue_description:
        description: description of the case
        type: string
      last_modified:
        description: Date this resource was last modified
        format: date-time
        readOnly: true
        type: string
      level1:
        description: level1 details of case
        readOnly: true
        type: string
      level2:
        description: level2 details of case
        readOnly: true
        type: string
      origin:
        description: where the record originated from - 'Web'
        readOnly: true
        type: string
      seller_email:
        description: email address of the user opening the case
        type: string
      seller_id:
        description: The seller's Target+ id
        readOnly: true
        type: string
      seller_order_number:
        description: id of the order under dispute
        type: string
      status:
        description: status of the case
        enum:
        - 19a Letter Sent
        - Assigned
        - Awaiting Screening
        - Cancelled
        - Closed
        - Escalated
        - In Progress
        - More Information Needed
        - New
        - On Hold
        - Partnering at HQ
        - Pending Guest
        - Pending Media
        - Released
        - Resolved
        - Resolved - Audit Complete
        - Resolved - Pending Audit
        - Resolved - Updated
        - Resolved - Update Required
        - Unprocessed
        readOnly: true
        type: string
      subject:
        description: subject of the case
        type: string
      vendor_id:
        description: the vmmId of the seller
        readOnly: true
        type: string
    type: object
  case_attachment:
    example:
      attachment_id: '1'
      transaction_id: '2'
      url: https://target.com
    properties:
      attachment_id:
        description: Id for the attachment
        type: string
      transaction_id:
        description: Transaction id for the upload
        type: string
      url:
        description: Url to get the attachement file
        type: string
    type: object
  case_email:
    example:
      attachment_id: 1234567-image.png
      created: '2020-05-18T13:00:15Z'
      from_address: first.last@target.com
      from_name: First Last
      last_modified: '2020-05-18T13:00:15Z'
      subject: My Case
      text_body: My case body
      to_address: first.last@target.com
    properties:
      attachment_id:
        description: id of email attachment
        type: string
      created:
        description: Date this resource was created
        format: date-time
        readOnly: true
        type: string
      from_address:
        description: from email address
        type: string
      from_name:
        description: from email name
        type: string
      last_modified:
        description: Date this resource was last modified
        format: date-time
        readOnly: true
        type: string
      subject:
        description: subject of email
        type: string
      text_body:
        description: body of email
        type: string
      to_address:
        description: to email address
        type: string
    type: object
  case_list:
    description: List of cases
    items:
      $ref: '#/definitions/case'
    type: array
  contact_info:
    example:
      address1: 1000 Nicollet Mall
      address2: Suite 100
      city: Minneapolis
      country_code: US
      email: steve.smith@target.com
