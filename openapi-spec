  distribution_center:
    example:
      address:
        address1: 1000 Nicollet Mall
        address2: Suite 100
        city: Minneapolis
        country_code: US
        postal_code: '55403'
        state: MN
      days_of_operation:
        days_closed:
        - '2020-01-01'
        - '2020-07-04'
        working_hours:
        - close_time: '15:30'
          day: MONDAY
          open_time: 09:00
      ecom_id: 123
      id: nrunkx
      name: Main distribution center
      ship_nodes:
      - building_lead_time_id: 6165
        carriers:
        - id: 4395
        - id: 4394
        created: '2019-10-15T19:43:25.925Z'
        created_by: user@email.com
        id: dvhuf3
        last_modified: '2024-01-09T22:25:14.843Z'
        last_modified_by: user@email.com
        max_orders_per_day: 100
        name: Z002TQJ-10-15-2019T14:43:05
        service_levels:
        - building_lead_time_id: 6157
          cut_off_time: 03:30:00
          generic_time_in_transit: 1
          id: 6220,
          last_adjusted: '2020-08-22T05:02:53.553Z'
        - building_lead_time_id: 6157
          cut_off_time: '17:00:00'
          generic_time_in_transit: 3
          id: 6222
          last_adjusted: '2020-08-22T05:02:53.553Z'
        vmm_id: OB52
      timezone: America/Chicago
      two_day_state_codes:
      - MN
      - IA
      vmm_id: 234623
    properties:
      address:
        $ref: '#/definitions/address'
      created: *id001
      created_by: *id002
      days_of_operation:
        $ref: '#/definitions/days_of_operation'
      ecom_id:
        description: ecom ID
        type: string
      id:
        description: Unique ID for this distribution center
        readOnly: true
        type: string
      last_modified: *id003
      last_modified_by: *id004
      name:
        description: Name of this distribution center
        type: string
      ship_nodes:
        description: Ship nodes
        items:
          $ref: '#/definitions/ship_node'
        readOnly: true
        type: array
      timezone:
        description: A timezone region
        example: America/Chicago
        type: string
      two_day_state_codes:
        description: List of ISO 3166-2:US state codes where two day shipping is available
          from this distribution center.
        items:
          example: US-MN
          type: string
        type: array
      vmm_id:
        description: VMM ID for this distribution center
        readOnly: true
        type: integer
    required:
    - name
    - address
    - timezone
    - days_of_operation
    type: object
