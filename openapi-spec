        - WOMAN
        - WOMAN_OTHER
        - BIPOC
        - LGBTQ
        - VETERAN
        - SERVICE_DISABLED_VET
        - SMALL_BUSINESS
        - SMALL_DISADVANTAGED_BUSINESS
        - DISADVANTAGED_BUSINESS_ENTERPRISE
        - DIFFERENTLY_ABLED
        - HUB_ZONE
        - ALASKAN_NATIVE
        type: string
      ethnicity:
        description: Ethnicity is required if the answer is yes to BIPOC question.
        type: string
      id:
        description: Unique identifier for the answer
        readOnly: true
        type: string
      last_modified: *id003
      last_modified_by: *id004
      seller_id:
        description: Identifier for the seller this answer is from
        readOnly: true
        type: string
    required:
    - diversity_question
    type: object
