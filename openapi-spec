        description: Reason for a cancellation
        enum:
        - OTHER
        - GENERAL_ADJUSTMENT
        - ITEM_NOT_AVAILABLE
        - CUSTOMER_RETURNED_ITEM
        - COULD_NOT_SHIP
        - ALTERNATE_ITEM_PROVIDED
        - BUYER_CANCELED
        - CUSTOMER_EXCHANGE
        - MERCHANDISE_NOT_RECEIVED
        - SHIPPING_ADDRESS_UNDELIVERABLE
        type: string
      order_line_number:
        description: Number for this order line
        readOnly: true
        type: string
      order_line_statuses:
        description: List of statuses for each state
        items:
          $ref: '#/definitions/order_line_status_update'
        type: array
    type: object
  order_list:
    description: List of order objects
    items:
      $ref: '#/definitions/order_display'
    type: array
  order_status_object:
    example:
      status: REFUNDED
    properties:
      status: *id027
    type: object
  payment_account:
    description: Payment details for a given account
    example:
      balance: 1000.99
      needs_verification: true
      seller_id: abc123
      verification_fields:
      - Field1
      - Field2
    properties:
      balance:
        description: The payment balance for this account.
        type: number
      needs_verification:
        description: Whether this account needs additional verification inside Stripe.
        type: boolean
      seller_id:
        description: ID for this seller in SMS
        type: string
      verification_fields:
        description: List of fields that need to be updated to pass verification.
        items:
          type: string
        type: array
    type: object
  payment_settings:
    description: Payout schedule settings for Stripe
    example:
      interval: weekly
      payment_day: Monday
    properties:
      interval:
        description: Interval for Stripe payouts
        readOnly: true
        type: string
      payment_day:
        description: Payment day for Stripe payouts
        readOnly: true
        type: string
    type: object
  payout:
    description: Payout object coming from payments processor
    example:
      arrival_date: '2020-05-15T18:56:52.000Z'
      balance_transaction: tr_123
      created_date: '2020-05-15T18:56:52.000Z'
      currency: USD
      description: Your payout
      destination_account: acc_456
      failure_balance_transaction: tr_789
      failure_code: '101112'
      id: abc123
      payout_amount: 1000.99
      status: COMPLETE
      type: Payout
    properties:
      arrival_date:
        description: Date the payout is expected to arrive in the bank. This factors
          in delays like weekends or bank holidays.
        format: date-time
        type: string
      balance_transaction:
        description: ID of the balance transaction that describes the impact of this
          payout on your account balance.
        type: string
      created_date:
        description: Date the payout was created.
        format: date-time
        type: string
      currency:
        description: Currency used (USD,EUR... ISO 4217)
        enum:
        - USD
        - GBP
        - YEN
        type: string
      description:
        description: Description for this payout
        type: string
      destination_account:
        description: ID of the bank account or card the payout was sent to.
        type: string
      failure_balance_transaction:
        description: If the payout failed or was canceled, this will be the ID of
          the balance transaction that reversed the initial balance transaction.
        type: string
      failure_code:
        description: Error code explaining reason for payout failure if available.
        type: string
      failure_message:
        description: Message to user further explaining reason for payout failure
          if available.
        type: string
      id:
        description: Payout ID. Also can use this for paging using the results_offset_id
          parameter.
        type: string
      payout_amount:
        description: Amount transferred.
        type: number
      status:
        description: Percentage going to target
        type: string
      type:
        description: Current status of the payout (paid, pending, in_transit, canceled
          or failed)
        type: string
    type: object
  payout_list:
    description: List of payout objects
    items:
      $ref: '#/definitions/payout'
    type: array
  performance_modifier:
