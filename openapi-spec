  order:
    example:
      created: '2020-05-06T19:34:01.963Z'
      created_by: steve.smith@target.com
      currency: USD
      distribution_center_id: 456def
      id: 123abc
      last_modified: '2020-05-06T19:34:01.963Z'
      last_modified_by: steve.smith@target.com
      order_date: '2020-05-06T19:34:01.963Z'
      order_lines:
      - is_two_day_ship: true
        order_line_number: '1'
        order_line_statuses: &id028
        - quantity: 1
          status: SHIPPED
        other_fees: 0.0
        other_info: &id029
        - name: Size
          value: Medium
        quantity: 1
        routing: r1234
        tcin: '12345678'
        total_gift_option_price: 0.0
        total_handling_price: 0.6
        total_item_discount: 0.0
        total_price: 10.0
        total_shipping_discount: 0.0
        total_shipping_price: 2.0
        unit_price: 10.99
      order_number: 789ghi
      order_status: RELEASED_FOR_SHIPMENT
      other_info:
      - name: Size
        value: Medium
      requested_delivery_date: '2020-05-06T19:34:01.963Z'
      requested_shipment_date: '2020-05-06T19:34:01.963Z'
      seller_id: 456def
      ship_advice_number: 101112jkl
      ship_node_id: 789ghi
      shipping_address:
        address1: 1000 Nicollet Mall
        address2: Suite 100
        city: Minneapolis
        country_code: US
        postal_code: '55403'
        state: MN
      vmm_vendor_id: '123'
    properties:
      created: *id001
      created_by: *id002
      currency:
        description: Currency used (USD,EUR... ISO 4217)
        enum:
        - USD
        - GBP
        - YEN
        type: string
      distribution_center_id:
        description: DC the package was shipped from
        type: string
      id:
        description: Internal order ID
        readOnly: true
        type: string
      last_modified: *id003
      last_modified_by: *id004
      order_date:
        description: Order date including time and timezone
        format: date-time
        type: string
      order_lines:
        description: List of products in this order
        items:
          $ref: '#/definitions/order_line'
        type: array
      order_number:
        description: Order number from OMS
        type: string
      other_info:
        description: Other details
        items:
          $ref: '#/definitions/product_property'
        type: array
      requested_delivery_date:
        description: Date the package should be delivered
        format: date-time
        type: string
      requested_shipment_date:
        description: Date the package should be shipped
        format: date-time
        type: string
      seller_id:
        description: Internal ID for seller
        readOnly: true
        type: string
      ship_advice_number:
        description: Ship advice number from OMS
        type: string
      ship_node_id:
        description: Ship node ID
        type: string
      shipping_address:
        $ref: '#/definitions/contact_info'
      status: &id027
        description: any order status
        enum:
        - PENDING
        - RELEASED_FOR_SHIPMENT
        - ACKNOWLEDGED_BY_SELLER
        - PARTIALLY_SHIPPED
        - SHIPPED
        - CANCELED
        - REFUNDED
        type: string
      vmm_vendor_id:
        description: VMM vendor for this the order
        type: string
    required:
    - vmm_vendor_id
    - order_number
    - ship_advice_number
    - distribution_center_id
    - ship_node_id
    - order_lines
    - shipping_address
    type: object
