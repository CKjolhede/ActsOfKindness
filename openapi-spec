  dispute_response:
    description: Dispute auto validation response
    enum:
    - MISSING_RETURN
    - MIN_RETURN_DATE
    - MIN_RETURN_DATE_15
    - MAX_RETURN_DATE
    - MAX_RETURN_DATE_45
    - MAX_RETURN_DATE_105
    - INVALID_QUANTITY
    - NO_CHARGE_BACK
    - MIN_CHARGE_BACK
    - DESTROY_PHYSICAL_DISP
    - TRACKING_EXISTS
    - RETURN_QUANTITY_EXCEEDED
