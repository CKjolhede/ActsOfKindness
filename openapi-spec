  legalization:
    description: Item types and brands a seller can legally sell
    example:
      created: '2020-05-06T19:34:01.963Z'
      created_by: steve.smith@target.com
      id: abc123
      last_modified: '2020-05-06T19:34:01.963Z'
      last_modified_by: steve.smith@target.com
      paired_attributes: &id021
      - id: 123abc
        type: BRAND
      primary_attribute: &id022
        id: 123abc
        type: BRAND
      seller_id: 123abc
    properties:
      created: *id001
      created_by:
        description: ID of this resource's creator
        readOnly: true
        type: string
      id:
        description: ID for the legalization
        readOnly: true
        type: string
      last_modified: *id003
      last_modified_by: *id004
      paired_attributes:
        description: Further restriction on legally sellable items. Cannot contain
          attributes with the same type as the primary attribute.
        items:
          description: Legalization attribute defines items a seller can or in the
            case of "Protected Legalization" can not sell
          example: &id017
            id: 123abc
            type: BRAND
          properties: &id018
            id:
              description: Unique identified for the legalization
              type: string
            type:
              description: Type of a legalizaion object
              enum: &id020
              - ITEM_TYPE
              - BRAND
              type: string
          required: &id019
          - type
          - id
          type: object
        type: array
      primary_attribute:
        description: Legalization attribute defines items a seller can or in the case
          of "Protected Legalization" can not sell
        example: *id017
        properties: *id018
        required: *id019
        type: object
      seller_id:
        description: id of a seller owning the legalization
        readOnly: true
        type: string
    required:
    - primary_attribute
    type: object
  legalization_attribute:
    description: Legalization attribute defines items a seller can or in the case
      of "Protected Legalization" can not sell
    example: *id017
    properties: *id018
    required: *id019
    type: object
  legalization_list:
    description: List of legalization objects
    items:
      $ref: '#/definitions/legalization'
    type: array
  legalization_status:
    description: Whether a seller is legalized to sell a product given its item type
      id and/or brand id
    enum:
    - VALID
    - NOT_VALID
    type: string
  legalization_type:
    description: Type of a legalizaion object
    enum: *id020
    type: string
  legalization_update:
    description: Item types and brands a seller can legally sell
    example:
      paired_attributes: *id021
      primary_attribute: *id022
    properties:
      paired_attributes:
        description: Further restriction on legally sellable items. Cannot contain
          attributes with the same type as the primary attribute.
        items:
          description: Legalization attribute defines items a seller can or in the
            case of "Protected Legalization" can not sell
          example: *id017
          properties: *id018
          required: *id019
          type: object
        type: array
      primary_attribute:
        description: Legalization attribute defines items a seller can or in the case
          of "Protected Legalization" can not sell
        example: *id017
        properties: *id018
        required: *id019
        type: object
    required:
    - primary_attribute
    type: object
  listing_error:
    description: Listing error that can be applied to a product
    example:
      created: '2020-05-06T19:34:01.963Z'
      created_by: steve.smith@target.com
      error_category: IMAGES
      error_code: '200'
      error_severity: CRITICAL
      last_modified: '2020-05-06T19:34:01.963Z'
      last_modified_by: steve.smith@target.com
      reason: Could not load
      status: ACTIVE
    properties:
      created: *id001
      created_by: *id002
      error_category: &id023
