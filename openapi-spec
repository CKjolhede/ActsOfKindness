    description: List of distribution center objects
    items:
      $ref: '#/definitions/distribution_center'
    type: array
  diversity_answer:
    example:
      answer: true
      created: '2020-05-06T19:34:01.963Z'
      created_by: steve.smith@target.com
      divesity_question: LGBTQ
      id: 123abc
      last_modified: '2020-05-06T19:34:01.963Z'
      last_modified_by: steve.smith@target.com
      seller_id: 123abc
    properties:
      answer:
        description: Answer for question true/false
        type: boolean
      certification:
        $ref: '#/definitions/diversity_certification'
      created: *id001
      created_by: *id002
      diversity_question:
        description: Identifies the quest this answer is for
