      - created: '2020-05-06T19:34:01.963Z'
        created_by: steve.smith@target.com
        current: true
        errors:
        - category: DATA_VALIDATION
          error_code: 1
          error_severity: NON_CRITICAL
          field_name: field1
          reason: Bad data
        id: 123abc
        last_modified: '2020-05-06T19:34:01.963Z'
