        description: Relationship type for this product. VAP is a parent, VC is a
          child and SA is stand-alone.
        enum:
        - VAP
        - SA
        type: string
      seller_id:
        description: Seller ID for this product
        readOnly: true
        type: string
      tcin:
        description: Internal Target Corporation Item Number (TCIN) for this product
        readOnly: true
        type: string
    type: object
  product_error:
    example:
      category: DATA_VALIDATION
      error_code: 1
      error_severity: NON_CRITICAL
      field_name: field1
      partner_action: Fix the value and resubmit
      reason: Bad data
    properties:
