  order_line:
    example:
      is_two_day_ship: true
      order_line_number: '1'
      order_line_statuses: *id028
      other_fees: 0.0
      other_info: *id029
      quantity: 1
      routing: r1234
      tcin: '12345678'
      total_gift_option_price: 0.0
      total_handling_price: 0.6
      total_item_discount: 0.0
      total_price: 10.0
      total_shipping_discount: 0.0
      total_shipping_price: 2.0
      unit_price: 10.99
    properties:
      cancellation_reason:
        description: Reason for a cancellation
        enum:
        - OTHER
        - ITEM_NOT_AVAILABLE
        - COULD_NOT_SHIP
        - SHIPPING_ADDRESS_UNDELIVERABLE
        type: string
      discounts:
        description: List of discounts applied to this order
        items:
          $ref: '#/definitions/order_discount'
        readOnly: true
        type: array
      external_id:
        description: Partner SKU value
        readOnly: true
        type: string
      is_two_day_ship:
        description: Flag indicating if the order is a two day ship order.
        type: boolean
      order_line_statuses:
        description: List of statuses for each state
        items:
          $ref: '#/definitions/order_line_status'
        type: array
      other_fees:
        description: Other fees
        format: float
        readOnly: true
        type: number
      other_info:
        description: Other details
        items:
          $ref: '#/definitions/product_property'
        readOnly: true
        type: array
      quantity:
        format: int64
        readOnly: true
        type: integer
      routing:
        description: Routing ID coming from OMS
        readOnly: true
        type: string
      tcin:
        description: Target Corporation Item Number (TCIN) for the item
        readOnly: true
        type: string
      total_gift_option_price:
        description: Wrapping costs
        format: float
        readOnly: true
        type: number
      total_handling_price:
        description: Total handling price
        format: float
        readOnly: true
        type: number
      total_item_discount:
        description: Total discount or promo for this item
        format: float
        readOnly: true
        type: number
      total_price:
        description: 'Total price (Must follow: totalPrice = SUM[quantity * unitPrice]
          + totalShippingPrice + (totalGiftOptionPrice + otherFees - totalOrderDiscount
          - totalShippingDiscount)) Note: Total price does not include any tax.

          '
        format: float
        readOnly: true
        type: number
      total_shipping_discount:
        description: Discount for shipping
        format: float
        readOnly: true
        type: number
      total_shipping_price:
        description: Price for shipping
        format: float
        readOnly: true
        type: number
      unit_price:
        description: Price paid for the product (2 decimal places)
        format: float
        readOnly: true
        type: number
    type: object
  order_line_entry:
    description: An entry in the order line
    example:
      cancellation_code: '1'
      cancellation_reason: No longer wanted
      order_line_number: '1'
      order_line_statuses:
        SHIPPED: 1
      quantity: 1
      tcin: '12345678'
