  /sellers/{seller_id}/products_catalog:
    get:
      description: Used to pull product catalog for a seller. Use last_modified parameter
        to filter for products that have been recently updated. This endpoint allows
        for deep paging via the after_id parameter. Normal paging via a page parameter
        is not supported on this endpoint however you can supply the per_page parameter
        to fetch more than 10 products per call. It is recommended to set per_page=200.
      parameters:
      - $ref: '#/parameters/seller_id'
      - $ref: '#/parameters/expand_products'
      - $ref: '#/parameters/fields'
      - $ref: '#/parameters/last_modified_query'
      - $ref: '#/parameters/latest_listing_status'
      - $ref: '#/parameters/tcin_optional'
      - $ref: '#/parameters/external_id'
      - $ref: '#/parameters/after_id'
      - description: Page size. We recommend setting this value to at least 200.
        in: query
        name: per_page
        required: false
        type: integer
      - $ref: '#/parameters/api_key'
      - $ref: '#/parameters/x_seller_id'
      - $ref: '#/parameters/seller_token'
      responses:
        200:
          description: Returned successfully.
          headers:
            Link:
              description: Relative link to the next page of results. Unlike typical
                paged endpoints, the link will contain a reference to the next page
                using the last ID from the current page in the after_id query parameter.
                No first, prev, or last relative links will be included.
              type: string
            X-Total-Count:
              description: Count of total results. This header is only present when
                after_id is not specified.
              type: integer
          schema:
            $ref: '#/definitions/product_list'
        400: *id076
        401: *id077
        403: *id078
        404: *id079
        500: *id080
      summary: Get product catalog
